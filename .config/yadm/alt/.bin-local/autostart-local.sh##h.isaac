#!/bin/bash

~/.screenlayout/screen.sh && sleep 1

$HOME/.bin/autostart-global.sh &

function run {
  if ! pgrep -f $1 ;
  then
    $@&
  fi
}

run discord --start-minimized
run flameshot
run numlockx
#run openrgb -p tralce
run pa-applet
run picom -cCGb -m 0.95 -i 0.95 --vsync --backend glx
un steam -silent
#run synergys --config ~/Scripts/OtherEssentialFiles/eli/synergy.sgc
run xbindkeys

(while true;do razer.py;sleep 15;done) &
